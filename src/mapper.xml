<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis1">
	<select id="showCreateTable" parameterType="String" resultType="java.util.HashMap">
		show create table ${value}
	</select>
	<update id="createTable1" parameterType="String">
		${value}
	</update>
	
	<update id="dropTable1" parameterType="String">
		drop table if exists ${value}
	</update>
</mapper>
